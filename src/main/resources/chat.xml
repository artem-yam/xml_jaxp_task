<?xml version='1.0' encoding='UTF-8' ?>
<chat xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xmlns="http://www.epam.com/jtc/chat"
      xsi:schemaLocation="http://www.epam.com/jtc/chat chat.xsd">
    <Message>
        <user_from>User1</user_from>
        <time_stamp>2002-05-30T09:20:10</time_stamp>
        <message>hi</message>
        <status>MESSAGE</status>
    </Message>
    <Message>
        <user_from>Admin1</user_from>
        <time_stamp>2002-05-30T09:30:10</time_stamp>
        <message>bye</message>
        <status>LOGIN</status>
    </Message>
    <Status>
        <title>LOGIN</title>
        <description>Юзер залогинился в чат</description>
    </Status>
    <Status>
        <title>MESSAGE</title>
        <description>Юзер оставил сообщение</description>
    </Status>
    <Status>
        <title>KICK</title>
        <description>Юзер выкинул из чата другого юзера указанного в поле
            message
        </description>
    </Status>
    <Status>
        <title>LOGOUT</title>
        <description>Юзер вышел из чата</description>
    </Status>
    <Role>
        <title>ADMIN</title>
        <description>Роль администратора</description>
    </Role>
    <Role>
        <title>USER</title>
        <description>Роль пользователя</description>
    </Role>
    <User>
        <nick>User1</nick>
        <role>USER</role>
    </User>
    <User>
        <nick>Admin1</nick>
        <role>ADMIN</role>
    </User>

</chat>
